# Supabase Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control

VITE_SUPABASE_PROJECT_ID="your-project-id-here"
VITE_SUPABASE_PUBLISHABLE_KEY="your-supabase-anon-key-here"
VITE_SUPABASE_URL="https://your-project-id.supabase.co"
VITE_SUPABASE_ANON_KEY="your-supabase-anon-key-here"
SUPABASE_SERVICE_ROLE_KEY="your-supabase-service-role-key"
SUPABASE_JWT_SECRET="your-supabase-jwt-secret"
SUPABASE_REDIRECT_URL_APP="http://localhost:5173/auth/callback"
SUPABASE_REDIRECT_URL_DASHBOARD="http://localhost:4173/auth/callback"

# Admin Credentials for Root User Creation
# These are used by the Admin Setup page to create the initial root user
# Replace with your actual admin credentials
VITE_ADMIN_EMAIL="your-admin-email@example.com"
VITE_ADMIN_PASSWORD="your-secure-admin-password"

# Google OAuth Configuration
# Note: Google OAuth credentials (Client ID & Secret) are configured in Supabase Dashboard
# under Authentication > Providers > Google. No additional env vars needed here.
